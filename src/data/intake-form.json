{
  "fieldsets": [
    {
      "name": "contact-info",
      "legend": "Contact Info",
      "fields": [
        {
          "name": "email",
          "label": "Email",
          "attributes": {
            "required": true,
            "type": "email"
          }
        },
        {
          "name": "phone",
          "label": "Phone",
          "attributes": {
            "type": "tel"
          }
        }
      ]
    },
    {
      "name": "name",
      "legend": "Name",
      "fields": [
        {
          "name": "first",
          "label": "First",
          "attributes": {
            "required": true
          }
        },
        {
          "name": "middle",
          "label": "Middle, or initial"
        },
        {
          "name": "last",
          "label": "Last",
          "attributes": {
            "required": true
          }
        }
      ]
    },
    {
      "name": "address",
      "legend": "Address",
      "required": true,
      "fields": [
        {
          "name": "address",
          "label": "Address"
        },
        {
          "name": "city",
          "label": "City"
        },
        {
          "name": "state",
          "label": "State"
        },
        {
          "name": "zip",
          "label": "Zip"
        }
      ]
    },
    {
      "name": "other",
      "legend": "Other",
      "fields": [
        {
          "name": "cookies-today",
          "label": "Did you have cookies today?",
          "options": [
            {
              "defaultValue": "yes",
              "label": "Yes"
            },
            {
              "defaultValue": "no",
              "label": "No"
            }
          ],
          "attributes": {
            "type": "radio"
          }
        }
      ]
    }
  ]
}
